<div id="login-box" class="login-popup">
    <a href="#" class="close"></a>
    <form class="signin">
        <fieldset class="textbox">
            <label class="username"> <span>Korisnicko ime</span>
                <input
                    id="login_username" name="username" value="" type="text"
                    autocomplete="on" placeholder="ID" />

            </label> 
            <label class="password"> <span>Lozinka</span> 
                <input
                    id="login_password" name="password" value="" type="password"
                    placeholder="PWD" />

            </label>
            <button id="btn_LoginUser" class="submit button" type="button">Prijava!</button>
            <br />
            <button id="newReg" class="submit button" type="button">Nova registracija?</button>
        </fieldset>
    </form>
</div>
