<div id="reg-box" class="login-popup">
	<a href="#" class="close" style=""></a>
	<form class="signin">
		<fieldset class="textbox">
			<label class="username"> <span>Korisnicko ime</span>
			 	<input
					id="n_username" name="username" value="" type="text"
					autocomplete="on" placeholder="ID" />				
			</label> 
			<label class="password"> <span>Lozinka</span> 
				<input
					id="n_password" name="password" value="" type="password"
					placeholder="Lozinka" />				
			</label>
			<label class="username"> <span>Email</span>
			 	<input
					id="n_email" name="email" value="" type="text"
					autocomplete="on" placeholder="Email Adresa" />				
			</label> 
			<button id="btn_RegisterNewUser" class="submit button" type="submit">Potvrdi</button>
		</fieldset>
	</form>
</div>
